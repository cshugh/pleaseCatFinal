<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
      "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
   	<typeAlias type ="com.ssafy.model.dto.cat" 		alias ="cat"/>
   	<typeAlias type ="com.ssafy.model.dto.comment" 		alias ="comment"/>
   	<typeAlias type ="com.ssafy.model.dto.following_cat" 		alias ="following_cat"/>
   	<typeAlias type ="com.ssafy.model.dto.following_user" 		alias ="following_user"/>
   	<typeAlias type ="com.ssafy.model.dto.likes" 		alias ="likes"/>
   	<typeAlias type ="com.ssafy.model.dto.post" 		alias ="post"/>
   	<typeAlias type ="com.ssafy.model.dto.ranking" 		alias ="ranking"/>
   	<typeAlias type ="com.ssafy.model.dto.report" 		alias ="report"/>
   	<typeAlias type ="com.ssafy.model.dto.unlikes" 		alias ="unlikes"/>
   	<typeAlias type ="com.ssafy.model.dto.user" 		alias ="user"/>
   	<!-- <typeAlias type ="com.ssafy.model.dto.FoodPageBean" alias ="pagebean"/> -->

  </typeAliases>
    
  <mappers>
     <!--  <mapper url = ""/> 리소스 or URL -->  
     <mapper resource = "static/cat_query.xml"/>
     <mapper resource = "static/comment_query.xml"/>
     <mapper resource = "static/following_cat_query.xml"/>
     <mapper resource = "static/following_user_query.xml"/>
     <mapper resource = "static/likes_query.xml"/>
     <mapper resource = "static/post_query.xml"/>
     <mapper resource = "static/ranking_query.xml"/>
     <mapper resource = "static/report_query.xml"/>
     <mapper resource = "static/unlikes_query.xml"/>
     <mapper resource = "static/user_query.xml"/>
  </mappers>
   <!-- query 설정 끝 -->
</configuration>






